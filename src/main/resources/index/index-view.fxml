<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="150.0" prefWidth="300.0"
      xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.antsol.op.gui.market.index.IndexController">
    <VBox prefHeight="150.0" prefWidth="150.0">
        <Label prefHeight="30.0" prefWidth="150.0" text="name"/>
        <Label prefHeight="30.0" prefWidth="150.0" text="current value"/>
        <Label prefHeight="30.0" prefWidth="150.0" text="companies"/>
        <Button onAction="#onPlotButtonClick" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                text="plot"/>
    </VBox>
    <VBox prefHeight="150.0" prefWidth="150.0">
        <Label fx:id="nameLabel" prefHeight="30.0" prefWidth="150.0"/>
        <Label fx:id="currentValueLabel" prefHeight="30.0" prefWidth="150.0"/>
        <ListView fx:id="companyListView" prefHeight="30.0" prefWidth="150.0"/>
        <Button onAction="#onClearButtonClick" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                text="clear"/>
        <Button onAction="#onRemoveButtonClick" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0"
                text="delete"/>
    </VBox>
</HBox>
