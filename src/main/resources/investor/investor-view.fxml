<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="240.0" prefWidth="300.0"
      xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.put.solarsan.op.gui.investor.InvestorController">
    <VBox prefHeight="240.0" prefWidth="150.0">
        <Label prefHeight="30.0" prefWidth="150.0" text="first name" />
        <Label prefHeight="30.0" prefWidth="150.0" text="last name" />
        <Label prefHeight="30.0" prefWidth="150.0" text="budget" />
        <Label prefHeight="30.0" prefWidth="150.0" text="currency" />
        <Label prefHeight="30.0" prefWidth="150.0" text="equities" />
        <Label prefHeight="30.0" prefWidth="150.0" text="participations" />
      <Button onAction="#onBuyButtonClick" mnemonicParsing="false"
              prefHeight="30.0" prefWidth="150.0" text="Buy equity" />
        <Button onAction="#onBuyParticipationButtonClick" mnemonicParsing="false"
                prefHeight="30.0" prefWidth="150.0" text="Buy participation" />
    </VBox>
    <VBox prefHeight="240.0" prefWidth="150.0">
        <Label fx:id="firstNameLabel" prefHeight="30.0" />
        <Label fx:id="lastNameLabel" prefHeight="30.0" />
        <Label fx:id="budgetLabel" prefHeight="30.0" />
        <Label fx:id="currencyLabel" prefHeight="30.0" />
        <ListView fx:id="equityListView" prefHeight="30.0" />
        <ListView fx:id="participationListView" prefHeight="30.0" />
      <Button onAction="#onRemoveButtonClick" mnemonicParsing="false"
              prefHeight="30.0" prefWidth="150.0" text="Delete" />
    </VBox>
</HBox>
